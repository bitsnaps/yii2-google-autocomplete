    "require": {
        "php": ">7.1",
        "wearesho-team/google-autocomplete": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.3",
        "squizlabs/php_codesniffer": "^3.3"
    },
    "authors": [
        {
            "name": "Roman Varkuta",
